<Window x:Class="Util.Controls.WPFTest.Window6"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Util.Controls.WPFTest"
        xmlns:xly="clr-namespace:Util.Controls;assembly=Util.Controls"
        mc:Ignorable="d" Background="{StaticResource WindowBackground}" WindowStyle="None"
        Title="Window6" Height="450" Width="800">
    <Grid>
        <ContentControl x:Name="Page_Change" HorizontalAlignment="Left" Margin="73,121,0,0" VerticalAlignment="Top" Height="282" Width="709"/>
