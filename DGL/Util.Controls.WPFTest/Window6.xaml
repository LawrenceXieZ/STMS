            <Label Content="店铺管理" Foreground="White" HorizontalAlignment="Left" Margin="340,19,0,0" VerticalAlignment="Top" FontSize="24"/>
            <Label Content="欢迎您，管理员" Foreground="White" HorizontalAlignment="Left" Margin="10,51,0,0" VerticalAlignment="Top" FontSize="16"/>
            <TextBlock Height="Auto"
                       Margin="10,5,0,0"
                       x:Name="tbDateText"
                       Foreground="White"
                       FontWeight="Bold"
                       FontFamily="Arial"
                       FontSize="15" />
            <TextBlock Height="Auto"
                       Margin="0,5,10,0"
                       x:Name="tbTimeText"
                       Foreground="#ffa51f"
                       FontWeight="Bold"
                       FontFamily="Calibri"
                       FontSize="23" />
            <xly:FButton Margin="699,10,10,39" Width="Auto" Height="Auto" Background="#FF931E1E" IsDefault="True" MouseOverBackground="#FFC52929" Click="FButton_Click_Exit">退出</xly:FButton>
        </Grid>
    </Grid>
</Window>
